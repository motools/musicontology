	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
</rdf:Property>

<rdf:Property rdf:about="http://purl.org/ontology/mo/event_homepage" rdfs:label="event homepage" vs:term_status="stable" mo:level="2">
