      rdfs:label "conductor" ;
      rdfs:subClassOf foaf:Agent ;
      mo:level "2" ;
      owl:equivalentClass
              [ rdf:type owl:Restriction ;
                owl:onProperty mo:conducted ;
                owl:someValuesFrom mo:Performance
              ] ;
      vs:term_status "unstable" .

mo:CorporateBody
      rdf:type owl:Class ;
