      rdfs:isDefinedBy mo: ;
      rdfs:label "Release type" ;
      mo:level "1" ;
      vs:term_status "stable" .

mo:SACD
      rdf:type owl:Class ;
      rdfs:comment "Super Audio Compact Disc used as medium to record a musical manifestation." ;
      rdfs:isDefinedBy mo: ;
      rdfs:label "SACD" ;
