</rdf:Property> 

<rdf:Property rdf:about="http://purl.org/ontology/mo/discogs" rdfs:label="discogs" vs:term_status="stable" mo:level="1">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
