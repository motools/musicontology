      rdfs:isDefinedBy mo: ;
      rdfs:label "corporate body" ;
      rdfs:subClassOf foaf:Organization ;
      mo:level "1" ;
      vs:term_status "stable" .

mo:DAT
      rdf:type owl:Class ;
